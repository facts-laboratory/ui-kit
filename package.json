{
  "name": "ui-kit",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx run ${NAME}:serve",
    "nx": "nx",
    "generate:lib": "npm run generate:react && npm run setup-storybook && npm run copy-storybook-config && npm run setup-tailwind",
    "generate:react": "nx generate @nrwl/react:library ${NAME} --unitTestRunner=vitest --style=css --bundler=vite --compiler=swc --globalCss=true --importPath=@facts-kit/${NAME} --publishable",
    "setup-tailwind": "nx generate @nrwl/react:setup-tailwind ${NAME}",
    "setup-storybook": "nx generate @nrwl/storybook:configuration ${NAME} --uiFramework=@storybook/react --bundler=vite --configureCypress=false && nx generate @nrwl/react:stories --generateCypressSpecs=false --project=${NAME}",
    "copy-storybook-config": "rm -rf packages/${NAME}/.storybook && cp -R .storybook packages/${NAME}/",
    "lint": "nx run ${NAME}:lint",
    "build": "nx run ${NAME}:build",
    "test": "nx run ${NAME}:test",
    "build-storybook": "nx run ${NAME}:build-storybook",
    "storybook": "nx run ${NAME}:storybook",
    "version": "nx run-many --targets=version --parallel=false --all --releaseAs=${TYPE}",
    "version:pre-release": "nx run workspace:version --releaseAs=prerelease --preid=${TYPE}",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.13",
    "@babel/preset-typescript": "7.12.13",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jscutlery/semver": "^2.30.1",
    "@nrwl/eslint-plugin-nx": "15.8.7",
    "@nrwl/js": "15.8.7",
    "@nrwl/linter": "15.8.7",
    "@nrwl/react": "15.8.7",
    "@nrwl/storybook": "15.8.7",
    "@nrwl/vite": "15.8.7",
    "@nrwl/workspace": "15.8.7",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/builder-vite": "^0.2.6",
    "@storybook/core-server": "^6.5.15",
    "@storybook/react": "^6.5.15",
    "@svgr/webpack": "^6.1.2",
    "@swc/core": "^1.2.173",
    "@testing-library/react": "14.0.0",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^3.0.0",
    "@vitest/coverage-c8": "~0.25.8",
    "@vitest/ui": "^0.25.8",
    "autoprefixer": "10.4.13",
    "babel-loader": "8.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "jsdom": "~20.0.3",
    "ngx-deploy-npm": "^5.2.0",
    "nx": "15.8.7",
    "postcss": "8.4.21",
    "prettier": "^2.6.2",
    "react-test-renderer": "18.2.0",
    "tailwindcss": "3.2.7",
    "typescript": "~4.9.5",
    "url-loader": "^4.1.1",
    "vite": "^4.0.1",
    "vite-plugin-dts": "~1.7.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-tsconfig-paths": "^4.0.2",
    "vitest": "^0.25.8"
  }
}
